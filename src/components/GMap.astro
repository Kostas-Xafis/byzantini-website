---
export type Props = {
	link: string;
};
const { link } = Astro.props;
---

<iframe
	class="w-[900px] h-[400px] shadow-md shadow-gray-400 rounded-md border-2 border-solid border-red-900"
	referrerpolicy="same-origin"
	src=`https://www.google.com/maps/embed?${link}`
	allowfullscreen
	loading="lazy"
>
</iframe>
